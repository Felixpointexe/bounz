class portal{
  float x;
  float y;
  float portalWidth;
  float portalHeight;
  
  boolean locked = false;

  public portal(float x, float y, float portalWidth, float portalHeight) {
    this.x = x;
    this.y = y;
    this.portalWidth = portalWidth;
    this.portalHeight = portalHeight;
  }
  
}
